<a href="#!/servers/{{serverID}}/sections/{{sectionID}}/player/{{item.ratingKey}}">
	{{#if item.rating}}<span class="rating">{{starRating item.rating}}</span>{{/if}}
	<h3 class="poster-title">{{truncate item.title 40}}</h3>
</a>

<a class="poster-link" href="#!/servers/{{serverID}}/sections/{{sectionID}}/player/{{item.ratingKey}}">
	<img class="poster" data-original="{{transcodeImage item.thumb 200 200}}" src="data:image/png;base64," alt="{{item.title}}">
</a>

<div class="list-detail">
	<div class="metadata">
		{{#if item.originallyAvailableAt}}
			<span class="metadata-label">Release Date</span>{{formatDate item.originallyAvailableAt}}
		{{/if}}

		{{#if item.duration}}
			<span class="metadata-label">{{eq item.type "show" "Episode Duration" "Duration"}}</span>{{formatDuration item.duration}}
		{{/if}}
	</div>

	{{#if item.summary}}
		<div class="list-tagline">{{truncate item.summary 300}}</div>
	{{/if}}
</div>