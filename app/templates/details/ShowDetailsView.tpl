<h2>{{show.title}}</h2>

<div class="details-controls">
	<img class="poster" data-original="{{transcodeImage show.thumb 200 300}}" src="data:image/png;base64,">

	{{#if nextEpisode}}
		<a class="next-episode-btn btn btn-primary" href="#">
			<i class="icon-eye-open icon-white"></i> Watch Next Episode
		</a>
	{{/if}}

	<a class="btn btn-inverse" href="#!/servers/{{serverID}}/sections/{{sectionID}}/edit/{{show.ratingKey}}">
		<i class="icon-pencil icon-white"></i> Edit Metadata
	</a>
</div>


<div class="tagline metadata">
	<span class="rating">{{starRating show.rating}}</span>

	{{unwatchedCount show.leafCount show.viewedLeafCount "Unwatched Episodes" "Unwatched Episode"}}
</div>

<div class="metadata">
	{{#if show.originallyAvailableAt}}
		<span class="metadata-label">Start Date</span>{{formatDate show.originallyAvailableAt}}
	{{else}}
		{{#if show.year}}
			<span class="metadata-label">Year</span>{{show.year}}
		{{/if}}
	{{/if}}
	
	{{#if show.duration}}
		<span class="metadata-label">Episode Duration</span>{{formatDuration show.duration}}
	{{/if}}
		
	{{#if show.contentRating}}
		<span class="metadata-label">Rating</span>{{show.contentRating}}
	{{/if}}
</div>

{{#if show.studio}}
	<div class="metadata">
		<span class="metadata-label">Studio</span>{{show.studio}}
	</div>
{{/if}}

{{#if show.Genre}}
	<div class="metadata">
		<span class="metadata-label">Genre</span>{{truncateTagList show.Genre 150}}
	</div>
{{/if}}

{{#if show.summary}}
	<div class="summary">{{truncate show.summary 700}}</div>
{{/if}}

{{#if nextEpisode}}
	<h3 class="next-header">Next Episode</h3>
{{/if}}

<h3 class="seasons-header">Seasons</h3>